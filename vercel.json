{
	"version": 2,
	"builds": [
		{
			"src": "simple_survey/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.9",
				"installCommand": "python3 -m pip install --user -r requirements.txt",
				"buildCommand": "python3 manage.py collectstatic --noinput"
			}
		},
		{
			"src": "staticfiles/",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/media/(.*)",
			"dest": "/media/$1"
		},
		{
			"src": "/(.*)",
			"dest": "simple_survey/wsgi.py"
		}
	]
}
